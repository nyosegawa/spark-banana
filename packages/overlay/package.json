{
  "name": "spark-banana",
  "version": "0.1.5",
  "description": "Real-time UI annotation overlay for AI coding agents",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && node -e \"require('node:fs').copyFileSync('src/styles/overlay.css', 'dist/styles.css')\"",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "html2canvas": "^1.4.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  }
}
